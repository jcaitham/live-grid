{
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/*.js": {
			"when": "$(basename).tsx"
		},
		"**/**.js": {
			"when": "$(basename).ts"
		},
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		"**/*.map": true,
		"**/*.css": {
			"when": "$(basename).scss"
		}
	}
}